{
    "name": "sgpopov/ironbox-client",
    "description": "IronBox REST client for PHP",
    "keywords": [
        "ironbox",
        "api",
        "rest-client",
        "secure-transfer"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "SGP\\IronBox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SGP\\IronBox\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
